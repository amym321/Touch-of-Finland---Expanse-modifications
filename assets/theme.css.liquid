  background-image:url({{ "/cdn.shopify.com/s/assets/gift-card/icon-print-164daa1ae32d10d1f9b83ac21b6f2c70.png" | asset_url }});
  background-repeat:no-repeat;
  background-position:0 0;
}

.giftcard__footer{
  text-align:center;
  padding:60px 0;
}

#QrCode img{
    padding:30px;
    border:1px solid;
    border-color:{{ settings.color_borders | default: "#1c1d1d" }};
    border-color:var(--colorBorder);
    border-radius:4px;
    margin:0 auto 30px;
  }

@media only screen and (max-width:768px){
  .giftcard{
    font-size:12px;
  }

  .giftcard-header{
    padding:30px 0;
  }

  .header-logo{
    font-size:2em;
  }

  .giftcard__border{
    padding:15px;
  }

  .giftcard__actions{
    padding:15px;
  }

  .giftcard__actions .btn{
    width:100%;
    padding-left:0;
    padding-right:0;
  }

  .template-giftcard .action-link{
    display:none;
  }
}

@media screen and (max-width:400px){
  .giftcard__amount strong{
    text-shadow:2px 2px 0 rgba(0, 0, 0, 0.1);
  }

  .giftcard__wrap:after,.giftcard__wrap:before{
    display:none;
  }

  .giftcard__code{
    font-size:.75em;
  }

  .giftcard__code--medium{
    font-size:.65em;
  }

  .giftcard__code--small{
    font-size:.55em;
  }
}

@media screen and (max-height:800px){
  .header-logo img{
    max-height:90px;
  }
}

@media print{
  @page{
    margin:0.5cm;
  }

  h2,h3,p{
    orphans:3;
    widows:3;
  }

  h2,h3{
    page-break-after:avoid;
  }

  body,html{
    background-color:#fff;
  }

  .giftcard-header{
    padding:10px 0;
  }

  .giftcard__border,.giftcard__content{
    border:0 none;
  }

  .add-to-apple-wallet,.giftcard__actions,.giftcard__wrap:after,.giftcard__wrap:before,.site-header__logo-link img:nth-child(2),.tooltip{
    display:none;
  }

  .giftcard__title{
    float:none;
    text-align:center;
  }

  .giftcard__code__text{
    color:#555;
  }

  .template-giftcard .shop-url{
    display:block;
  }

  .template-giftcard .logo{
    color:#58686f;
  }
}

/*** CUSTOM STYLES ***/
.register_accepts-mktg {
  display: flex;
  align-items: center;
  margin-bottom: 30px;
}
    
.register_accepts-mktg input,
.register_accepts-mktg label {
  font-weight: normal;
}
    
.site-nav__link.sale-link,
.slide-nav__link .sale-link{
  color:#941D1D !important;
}

    .see_price{
      text-decoration: line-through;
      text-decoration-color:#9b230f;
      float: left;    
    }
    .see_price_wrapper{
      position: relative;
      float: left;    
      font-size: 20px;
      margin-top: 7px;
      margin-left: 15px;
      font-weight: 700;
      text-transform: uppercase;
      color:#9b230f;
    }
    .grid-product__price .see_price_wrapper{
      font-size: 13px;
      margin: 0px;
      clear: both;     
    }
    .see_price_wrapper .see_price_popup{
      position: absolute;
      max-width: 250px;
      height: auto;
      font-size: 14px;
      color: #333;
      text-transform: initial;
      display: none;
      border: 1px solid #333;
      border-radius: 7px;
      padding: 7px;
      background: #fff;
      top: 35px;
    }
    .see_price_wrapper .see_price_popup:before{
      content: '';
      position: absolute;
      width: 10px;
      height: 10px;
      border-width: 1px 0px 0px 1px;
      top: -6px;
      left: 110px;
      border-style: solid;
      transform: rotate(45deg);
      background: #fff;
    }
    .see_price_wrapper img.question_mark:hover + .see_price_popup{
      display: block;
    }
    @media only screen and (max-width:768px){ 
    .grid-product__price .see_price_wrapper{
      font-size: 12px;
    }
    }
.product-grid .see_price_wrapper .question_mark{display: none;}

